{
  "name": "ci-cd",
  "version": "1.0.0",
  "main": "app.js",
  "description": "CI/CD automation system with Docker build, Git polling, queue management",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nodemon app.js",
    "docs": "jsdoc -c jsdoc.json",
    "docs:serve": "npm run docs && npx http-server docs/ -p 8080"
  },
  "keywords": [
    "ci-cd",
    "docker",
    "automation",
    "build",
    "deployment"
  ],
  "author": "KhanhNguyen",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.6.8",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nest": "^0.1.6",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prettier": "^3.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/pg": "^8.15.6",
    "jsdoc": "^4.0.5",
    "nodemon": "^3.1.9"
  }
}
